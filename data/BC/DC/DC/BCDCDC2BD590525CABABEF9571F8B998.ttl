    dc:title "A new species of Andrena (Trachandrena) from the Southwestern United States (Hymenoptera, Andrenidae)" ;
    bibo:endPage "103" ;
    bibo:journal "Journal of Hymenoptera Research" ;
    bibo:startPage "87" ;
    bibo:volume "77" ;
