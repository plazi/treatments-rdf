    dc:creator "Ferreira, Rodrigo Lopes", "Hellmann, Lindsey", "Leal-Zanchet, Ana Maria" ;
    dc:date "2018" ;
    dc:title "Two new sympatric troglobitic freshwater flatworms (Platyhelminthes: Dugesiidae) from a hotspot of subterranean biodiversity in the Neotropics" ;
    bibo:endPage "574" ;
    bibo:issue "3" ;
    bibo:journal "Zootaxa" ;
    bibo:pubDate "2018-06-22" ;
    bibo:startPage "561" ;
