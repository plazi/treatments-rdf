    dc:date "2021" ;
    dc:title "Molecular phylogenetics and taxonomic reassessment of the widespread agamid lizard Calotes versicolor (Daudin, 1802) (Squamata, Agamidae) across South Asia" ;
    bibo:endPage "696" ;
    bibo:journal "Vertebrate Zoology" ;
    bibo:pubDate "2021-10-20" ;
    bibo:startPage "669" ;
    bibo:volume "71" ;
